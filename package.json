{
  "name": "d3-sankey-circular",
  "version": "0.34.0",
  "description": "D3 sankey with circular links",
  "author": "Tom Shanley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomshanley/d3-sankey-circular.git"
  },
  "main": "dist/d3-sankey-circular.js",
  "module": "dist/d3-sankey-circular.es.js",
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-shape": "^1.2.0",
    "elementary-circuits-directed-graph": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.19.1",
    "mkdirp": "^0.5.1",
    "prettier": "^2.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:clean": "npm run clean && npm run build",
    "prepare": "npm run build:clean",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "d3"
  ],
  "bugs": {
    "url": "https://github.com/tomshanley/d3-sankey-circular/issues"
  },
  "homepage": "https://github.com/tomshanley/d3-sankey-circular#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 120,
    "trailingComma": "es5",
    "importOrder": [
      "^[./]"
    ],
    "importOrderSeparation": true
  }
}
